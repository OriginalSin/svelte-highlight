{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0
  }
}
