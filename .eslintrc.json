{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": ["svelte3"],
  "overrides": [
    {
      "files": ["src/**/*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ],
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0
  },
  "settings": {}
}
